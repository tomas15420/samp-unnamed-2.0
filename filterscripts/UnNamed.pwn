		}
		else if(Znaminko == 1)
		{
			format(str,sizeof(str),"Kdo první vypoèítá pøíklad "w"%d - "w"%d{0073E0}, získá: "g"%d bodù.",Priklad[0],Priklad[1],PostrehOdmena);
