	if(!IsPlayerOnHighestLevel(playerid))
	{
		if(!IPA(playerid)) return SM(playerid,"Nejsi pøihlášený pøes "r"RCON");
	}
	new id,query[200],Cache:cache;
	if(sscanf(params,"i",id)) return SM(playerid,"Použití: "r"/deletehouse [ è.p domu ]");
