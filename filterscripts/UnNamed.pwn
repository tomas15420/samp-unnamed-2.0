			{
				if(OnEvent[playerid] == true)
				{
			    	SetPlayerDrunkLevel(playerid,50000);
				}
			}
			if(IsPlayerInAnyVehicle(playerid))
			{
				if(AutRepair == 1 || AR[playerid] == true || GOD[playerid] == true)
				{
