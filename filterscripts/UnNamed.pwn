				if(InSumo[i] == true)
				{
					SetPlayerVirtualWorld(i,0);
					InSumo[i] = false;
					DestroyVehicle(SumoVehicle[i]);
					SumoVehicle[i] = 0;
					KillTimer(SumoTimer[i]);
