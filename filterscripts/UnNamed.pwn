stock IsVehicleEmpty(vehicleid)
{
 	for(new i; i <= GetPlayerPoolSize(); i++)
  	{
    	if(IsPlayerConnected(i) && IsPlayerInAnyVehicle(i) && GetPlayerVehicleID(i) == vehicleid) return 0;
  	}
  	return 1;
}

