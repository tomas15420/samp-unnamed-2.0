stock ConnectToStunt(playerid,stuntid)
{
	if(stuntid < 0 || stuntid > sizeof(StuntPos)) return SM(playerid,"Chyba, zkuste to znovu");
	JailCmdBlock(playerid);
	if(ViewMode[playerid]) return SM(playerid,"Teï se nemùžeš pøipojit do stuntu");
	if(PrisonTime[playerid]) return SM(playerid,"Ve vìzení nemùžeš plnit stunt");
	if(OnEvent[playerid] == true) return SM(playerid,"Na eventu nemùžeš plnit stunt");
	if(IsPlayerSupplyRun(playerid)) return SM(playerid,"Právì plníš misi, nemùžeš vykonat tuto akci");
	if(InDM[playerid] != -1) return SM(playerid,"V DM nemùžeš plnit stunt");
	if(WorkMission[playerid] > 0) return SM(playerid,"Už máš nìco na práci "r"/zrusmisi");
