	DestroyObject(ObjectOne[playerid]);
	DestroyObject(ObjectTwo[playerid]);
 	DestroyObject(ObjectThree[playerid]);
	DestroyObject(ObjectFour[playerid]);
	DestroyObject(ObjectFive[playerid]);
	DestroyVehicle(ACAR[playerid]);
