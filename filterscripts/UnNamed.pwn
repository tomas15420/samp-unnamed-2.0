							};
							color = colors[random(sizeof(colors))];
						}
					}
				}
				for(new i; i <= GetPlayerPoolSize(); i ++)
				{
				    if(IPC(i) && !IsPlayerNPC(i))
				    {
				        if(i != playerid)
				        {
				            if(Player[i][pHide] <= 0)
				            {
							    if(WorkMission[i] == 7)
