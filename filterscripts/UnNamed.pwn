stock Hash(password[])
{
	new promena0 = strlen(password); //z
	new promena1 = 1; //x
	new promena2 = 0; //q
	new promena3 = 0; //r
	for(promena3 = 0; promena3 < promena0; promena3++)
	{
		promena1 = (promena1 + password[promena3]) % 65521;
		promena2 = (promena2 + promena1) % 65521;
	}
	return promena1 + (promena2 << 16);
}

