forward FallOut();

public FallOut()
{
	new obj;
	for(new i; i < eventobjects*eventobjects; i ++)
	{
	    for(new x; x < eventpatra; x ++)
	    {
			if(Fallout[i][x][FDestroyed] == false)
				obj ++;
		}
	}
	if(obj > 0)
	{
		new rand = random(eventobjects*eventobjects),rand2 = random(eventpatra);
		while(Fallout[rand][rand2][FDestroyed] == true)
