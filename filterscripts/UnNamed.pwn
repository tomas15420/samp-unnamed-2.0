		}
	}
	format(str,sizeof(str),"+ %d score",score);
	DMScore[playerid] += score;
	PlayerTextDrawSetString(playerid,Textdraw36[playerid],str);
	PlayerTextDrawShow(playerid,Textdraw36[playerid]);
	format(str,sizeof(str),"~r~- %d score",5);
