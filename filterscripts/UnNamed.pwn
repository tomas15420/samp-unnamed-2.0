		    if(IPC(i))
		        if(IsPlayerHouseOwner(i,id))
		            PlayerHouse[i] = -1;
		        
		for(new i; HouseData:i < HouseData; i ++)
		{
		    House[id][HouseData:i] = 0;
		}
	}
	else SM(playerid,"Tento dùm není vytvoøen");
	cache_delete(cache,mysql);
	return 1;
