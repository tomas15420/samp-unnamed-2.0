	format(string,sizeof(str),"[ %02d.%02d.%d %02d:%02d:%02d ] %s\n",day,month,year,hour,minute,second,text);
	if(Soubor)
	{
		fwrite(Soubor,string);
		fclose(Soubor);
	}
