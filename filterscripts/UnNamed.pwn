stock EncodeNick(nick[])
{
	new pName[24+1];
	format(pName,sizeof(pName),nick);
	for(new i; i < strlen(pName); i ++)
	{
	    if(pName[i] == 'I') pName[i] = '#';
	    if(pName[i] == 'l') pName[i] = '#';
		if(pName[i] == ']') pName[i] = '#';
		if(pName[i] == '[') pName[i] = '#';
	}
	return pName;
}

