   		SaveBeforeEventData(playerid);
	if(pStunt[playerid][sStuntID] != stuntid)
	{
		format(str,sizeof(str),"%s ~g~%s ~w~zacal plnit ~r~stunt %d~w~.",Players(playerid),Jmeno(playerid),stuntid+1);
