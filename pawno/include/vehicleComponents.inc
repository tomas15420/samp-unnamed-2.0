stock GetComponentCategoryName(type)
{
	new name[30] = "Error";
	if(type < sizeof(ComponentType) && type >= 0)
	{
        format(name,sizeof(name),ComponentType[type]);
	}
	return name;
}


stock GetVehicleComponentCategoryName(comp)
{
	new name[30] = "Error";
	if(comp >= 1000 && comp < sizeof(Component)+1000)
	{
	    new type = GetVehicleComponentCategory(comp);
	    if(type >= 0 && type < sizeof(ComponentType))
	    {
	        format(name,sizeof(name),ComponentType[type]);
	    }
	}
	return name;
}

stock GetVehicleComponentCategory(comp)
{
	if(comp >= 1000 && comp < sizeof(Component)+1000)
		return Component[comp-1000][compType];
	return 0;
}

stock GetVehicleComponentName(comp)
{
	new name[30] = "Error";
	if(comp >= 1000 && comp < sizeof(Component)+1000)
		format(name,sizeof(name),Component[comp-1000][compName]);
	return name;
}

stock GetVehicleComponentPrice(comp)
{
	if(comp >= 1000 && comp < sizeof(Component)+1000)
		return Component[comp-1000][compPrice];
	return 0;
}


stock LoadVehicleComponentCategories(model,array[COMP_CAT_SIZE])
{
	if(model >= 400 && model < 611)
	{
		new category = -1;
		new idx;
		for(new i; i < COMP_CAT_SIZE; i ++)
		{
			array[i] = -1;
		}
		for(new i; i < MAX_VEH_COMPONENTS; i ++)
		{
			new comp = VehicleComponents[model-400][i];
			if(comp == INVALID_VEHICLE_COMPONENT_ID) break;
			new newCat = GetVehicleComponentCategory(comp);
			if(newCat != category && !IsArrayContainsNumber(newCat,array))
			{
				category = newCat;
				array[idx++] = category;
			}
		}
	}
}

stock LoadVehicleComponents(model,category,array[MAX_VEH_COMPONENTS])
{
	if(model >= 400 && model < 611 && category >= 0 && category < COMP_CAT_SIZE)
	{
		new idx = 0;
		for(new i; i < MAX_VEH_COMPONENTS; i ++)
		{
			new comp = VehicleComponents[model-400][i];
			if(comp == INVALID_VEHICLE_COMPONENT_ID || Component[comp-1000][compDisabled] == true) continue;

			if(GetVehicleComponentCategory(comp) == category){
				array[idx++] = comp;
			}
		}
	}
}

stock IsArrayContainsNumber(num,array[],len = sizeof(array))
{
	for(new i; i < len; i ++)
	{
		if(array[i] == num)
			return 1;
	}
	return 0;
}
